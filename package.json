{
  "name": "language-jedi",
  "version": "0.3.0",
  "description": "Jedi language support in Atom",
  "engines": {
    "atom": "*"
  },
  "main": "./lib/main",
  "configSchema": {
    "enableCMUISnippets": {
      "title": "CMUI Snippets",
      "description": "Enable classes snippets in CMUI (by autocomplete-plus).",
      "type": "boolean",
      "default": false
    }
  },
  "providedServices": {
    "autocomplete.provider": {
      "versions": {
        "2.0.0": "provide"
      }
    }
  },
  "dependencies": {},
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CyanSalt/atom-jedi.git"
  },
  "bugs": {
    "url": "https://github.com/CyanSalt/atom-jedi/issues"
  },
  "license": "MIT"
}
